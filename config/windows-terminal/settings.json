            "selectionBackground": "#ACB0BE",
            "cursorColor": "#DC8A78"
        }
    ],
    "actions": [
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": {
                "action": "newTab"
            },
            "keys": "ctrl+shift+t"
        },
        {
            "command": {
                "action": "closeTab"
            },
            "keys": "ctrl+shift+w"
        },
        {
            "command": "toggleFocusMode",
            "keys": "ctrl+shift+p"
        }
    ],
    "theme": "dark",
    "launchMode": "default",
    "startOnUserLogin": false,
    "confirmCloseAllTabs": true,
    "windowingBehavior": "useAnyExisting"
}
