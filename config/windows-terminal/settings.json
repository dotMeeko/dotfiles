{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "copyOnSelect": false,
    "copyFormatting": "none",
    "profiles": {
        "defaults": {
            "font": {
                "face": "JetBrainsMono NF",
                "size": 14,
                "weight": "normal"
            },
            "opacity": 85,
            "useAcrylic": true,
            "acrylicOpacity": 0.7,
            "colorScheme": "Catppuccin Mocha",
            "cursorShape": "bar",
            "antialiasingMode": "cleartype",
            "padding": "4, 4, 4, 4"
        }
    },
    "schemes": [
        {
            "name": "Catppuccin Mocha",
