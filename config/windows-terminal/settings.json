            "brightYellow": "#EED49F",
            "brightBlue": "#8AADF4",
            "brightPurple": "#F5BDE6",
            "brightCyan": "#8BD5CA",
            "brightWhite": "#A5ADCB",
            "background": "#24273A",
            "foreground": "#CAD3F5",
            "selectionBackground": "#5B6078",
            "cursorColor": "#F4DBD6"
        },
        {
            "name": "Catppuccin Frappe",
            "black": "#51576D",
            "red": "#E78284",
            "green": "#A6D189",
            "yellow": "#E5C890",
            "blue": "#8CAAEE",
            "purple": "#F4B8E4",
            "cyan": "#81C8BE",
            "white": "#B5BFE2",
